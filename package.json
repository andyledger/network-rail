{
  "private": true,
  "scripts": {
    "setup": "npm install && mix",
    "clean": "rm -rf public/scripts/*",
    "prestart": "npm run clean",
    "start": "mix watch",
    "prebuild": "npm run clean",
    "build": "mix",
    "lint": "npm run lint:js && npm run lint:css",
    "build:production": "npm run clean && mix --production",
    "clear": "wp acorn optimize:clear",
    "hot": "mix watch --hot",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "lint:js": "eslint resources/scripts",
    "translate": "npm run translate:pot && npm run translate:js",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --ignore-domain --include=\"app,resources\""
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "algoliasearch": "^4.9.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "azure-storage": "^2.10.3",
    "core-js": "^3.15.2",
    "flickity": "^2.2.2",
    "flickity-fade": "^1.0.0",
    "gmap-vue": "^1.5.0",
    "gulp": "^5.0.1",
    "iframe-resizer": "^4.3.6",
    "jquery": "^3.5.1",
    "laravel-mix-transpile-node-modules": "^2.0.1",
    "lodash": "^4.17.21",
    "lottie-web": "^5.7.11",
    "lozad": "^1.16.0",
    "moment": "^2.29.1",
    "promise-polyfill": "^8.2.0",
    "register-service-worker": "^1.7.2",
    "sortable-tablesort": "^2.0.0",
    "underscore": "^1.12.1",
    "vue": "^2.6.12",
    "vue-apollo": "^3.1.0",
    "vue-instantsearch": "^3.9.0",
    "vue-plain-pagination": "^0.3.0",
    "vue-server-renderer": "^2.6.12",
    "vue-slider-component": "^3.2.11",
    "vue-tippy": "^4.10.0",
    "vue2-transitions": "^0.3.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@tailwindcss/typography": "^0.4.0",
    "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@wordpress/babel-preset-default": "^5.0.1",
    "@wordpress/browserslist-config": "^3.0.1",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "https-browserify": "^1.0.0",
    "laravel-mix": "^6.0.11",
    "laravel-mix-polyfill": "^3.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.6",
